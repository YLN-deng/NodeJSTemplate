{
  "name": "typescriptnode",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "init-tsconfig": "tsc --init",
    "copy-static": "ts-node copyStatic.ts",
    "ts-build": "tsc",
    "tsc-alias": "tsc-alias",
    "dev": "cross-env NODE_ENV=development nodemon",
