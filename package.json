    "@types/request-ip": "^0.0.41",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
