    "build": "npm run ts-build && npm run tsc-alias && npm run copy-static"
  },
  "dependencies": {
    "class-validator": "^0.14.1",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "errorhandler": "^1.5.1",
    "express": "~4.16.1",
    "http-errors": "^2.0.0",
    "ioredis": "^5.3.2",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "memjs": "^1.3.2",
    "morgan": "~1.9.1",
    "mysql2": "^3.9.3",
    "rate-limiter-flexible": "^5.0.0",
    "request-ip": "^3.3.0",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/errorhandler": "^1.5.3",
